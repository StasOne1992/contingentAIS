{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.14",
        "dompdf/dompdf": "^2.0",
        "knplabs/knp-menu-bundle": "^3.2",
        "npub/gos": "^0.9.11",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpoffice/phpspreadsheet": "^1.29",
        "phpstan/phpdoc-parser": "^1.15",
        "stof/doctrine-extensions-bundle": "*",
        "symfony/asset": "7.*",
        "symfony/console": "7.*",
        "symfony/debug-bundle": "7.*",
        "symfony/doctrine-messenger": "7.*",
        "symfony/dotenv": "7.*",
        "symfony/expression-language": "7.*",
        "symfony/flex": "^2",
        "symfony/form": "7.*",
        "symfony/framework-bundle": "7.*",
        "symfony/http-client": "7.*",
        "symfony/intl": "7.*",
        "symfony/mailer": "7.*",
        "symfony/mercure-bundle": "^0.3.7",
        "symfony/messenger": "7.*",
        "symfony/mime": "7.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "7.*",
        "symfony/process": "7.*",
        "symfony/property-access": "7.*",
        "symfony/property-info": "7.*",
        "symfony/requirements-checker": "^2.0",
        "symfony/runtime": "7.*",
        "symfony/security-bundle": "7.*",
        "symfony/serializer": "7.*",
        "symfony/string": "7.*",
        "symfony/translation": "7.*",
        "symfony/twig-bundle": "7.*",
        "symfony/validator": "7.*",
        "symfony/web-link": "7.*",
        "symfony/webpack-encore-bundle": "2.*",
        "symfony/yaml": "7.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^3.5"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "App",
            "App\\StudentApp\\": "App/StudentApp",
            "App\\MainApp\\": "App/MainApp"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MainApp\\Tests\\": "tests/MainApp",
            "StudentApp\\Tests\\": "tests/StudentApp"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "requirements-checker": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "7.0.*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.0.*"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "7.*",
        "symfony/css-selector": "7.*",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "7.*",
        "symfony/stopwatch": "7.*",
        "symfony/web-profiler-bundle": "7.*"
    }
}
