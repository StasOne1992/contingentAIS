{% extends 'base.html.twig' %}

{% block title %}FamilyTypeList index{% endblock %}

{% block body %}
    <h1>FamilyTypeList index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for family_type_list in family_type_lists %}
            <tr>
                <td>{{ family_type_list.id }}</td>
                <td>{{ family_type_list.Name }}</td>
                <td>
                    <a href="{{ path('app_family_type_list_show', {'id': family_type_list.id}) }}">show</a>
                    <a href="{{ path('app_family_type_list_edit', {'id': family_type_list.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_family_type_list_new') }}">Create new</a>
{% endblock %}
